.foot_circle_hover{
  transform:scale(0);
}
.menu_circle_hover{
}
.top_time{
  color: #aeaeae;
  margin-left: 30rpx;
}
.center_circle{
  width: 100rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  background: #fdbb33;
  border-radius: 50%;
  position: fixed;
  font-size: 30rpx;
  left: 43vw;
  bottom: 150rpx; 
  transition:all 0.1s ;
}
.menu_circle{
  width: 50rpx;
  height: 50rpx;
  background: #fdbb33;
  border-radius: 50rpx;
  font-size: 12rpx;
  text-align: center;
  line-height: 50rpx;
  position: fixed;
  transform:scale(0);
  transition:all 0.1s ;
}
.menu_circle_hover .menu_circle{
  transition: transform 0.1s 0.08s, z-index 0s  0.5s;
  transform:scale(2);
  z-index: 1;
}
.menu_circle:nth-child(1){
  left: 30vw;
  bottom: 180rpx;  
  transition-delay: 0.02s;
}
.menu_circle:nth-child(2){
  left: 46vw;
  bottom: 290rpx;  
  transition-delay: 0.05s;
}
.menu_circle:nth-child(3){
  left: 62vw;
  bottom: 180rpx;  
  transition-delay: 0.10s;

}
.menu_circle:nth-child(4){
  left: 55vw;
  bottom: 60rpx;
  transition-delay: 0.18s;

}
.menu_circle:nth-child(5){
  left: 37vw;
  bottom: 60rpx;
  transition-delay: 0.25s;
}

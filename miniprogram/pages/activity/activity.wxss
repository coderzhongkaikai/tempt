
.box{
  position: relative;
}
.photo{
    display: flex;
    justify-content: center;
    margin: 20rpx 10rpx;
}
.bk_card{
  border: 10rpx solid #fdbb33;
}
.photo image{

}
.contain{
  width: 89vw;
  position: absolute;
  top: 81.8%;
  right: 0rpx;
  background: #fdf9f9;
  border-radius: 80rpx 80rpx 0 0;
  display: flex;
  border: 10rpx solid #fdbb33;
  border-bottom: none;
  flex-direction: column;
  justify-content: flex-start;
  padding: 34rpx ;
}
.head{
  display: flex;
  justify-content: space-between;
}
.head .icon-liulan ,.head text{
  margin: 5rpx;
  font-size: 28rpx;
}
.head i{
  font-size: 40rpx;
  color: #fdbb33;
}
.row{
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.contain .title{
  display: flex;
  justify-content: flex-start;
  margin-top: 30rpx;
  font-size: 33rpx;
  color: black;

}
.title .iconfont{
  line-height: 43rpx;
  margin-right: 10rpx;
  font-size: 35rpx;
  color: #fdbb33;
}
.contain .p{
  color: rgb(78, 78, 78);
  font-size: 30rpx;
}
.foot{
  font-size: 32rpx;
  margin:80rpx auto;
}
.creator_look{
  display: flex;
  justify-content: space-between;
}
.creator_look .del{
  color: red;
}
.creator_look text:active{
  text-decoration:underline;
}
.creator_look .change{
  color: #fdbb33;
}
.creator_look .icon{
  padding: 0rpx 30rpx;
}
.member_list{
  color: #499b86;
}
.footer_fix{
  position: absolute;
  bottom: 0rpx;
  height: 100rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}


.function_box{
  margin: 20rpx auto;

  display: flex;
  flex-direction: column;
}

.function_btn{
  margin-top: 40rpx;
  width: 300rpx;
  border: 1rpx solid #fdbb33;
  text-align: center;
  height: 70rpx;
  line-height: 70rpx;
  color: #000000;
  border-radius: 20rpx;
}
.del_color{
  color: red;
}
.foot_circle{
   width: 100rpx;
   height: 100rpx;
   background: #fdbb33;
    border-radius: 50rpx;
    transition:width 0.3s, height 0.3s, transform 0.3s;
    transition-duration: 0.3s;
}
.scroll{
  height: 100vh;
  overflow: hidden;
  z-index: -1;
}
.foot_menu{
 
  /* bottom: 50rpx;
  width: 100vw;
